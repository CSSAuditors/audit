{%- if report -%}
  <div class="table-wrapper">
    <table class="small">
      <thead>
        <tr>
          <th><b>Name</b></th>
        </tr>
      </thead>
      <tbody>
        {%- if report.errorList -%}
          {%- for item in report.errorList -%}
            <tr>
              <td>{{ item }}</td>
            </tr>
          {%- endfor -%}
        {%- endif -%}
      </tbody>
    </table>
  </div>
{%- endif -%}
